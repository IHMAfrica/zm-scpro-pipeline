{
  "name": "carepro-sqlserver-connector",
  "config": {
    "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector",
    "database.hostname": "${SQLSERVER_HOST}",
    "database.port": "${SQLSERVER_PORT}",
    "database.user": "${SQLSERVER_USER}",
    "database.password": "${SQLSERVER_PASSWORD}",
    "database.names": "${SQLSERVER_DB}",
    "topic.prefix": "carepro",
    "database.encrypt": "false",
    "include.schema.changes": "true",
    "table.include.list": "dbo.Clients,dbo.Encounters,dbo.Interactions,dbo.Medications,dbo.DispensedItems,dbo.PatientStatuses,dbo.Vitals,dbo.Investigations,dbo.Results,dbo.ARTServices,dbo.HTS,dbo.WHOConditions,dbo.Diagnoses,dbo.ConstitutionalSymptoms,dbo.IdentifiedConstitutionalSymptoms,dbo.IdentifiedTBSymptoms,dbo.TBSymptoms",
    "decimal.handling.mode": "string",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "true",
    "key.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter.schema.registry.url": "http://schema-registry:8081",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://schema-registry:8081"
  }
}